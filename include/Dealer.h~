#include "Player.h"
#include "Carta.h"

public class Dealer : public Player{
    private Carta cartaFechada;
    
    public Dealer::getCartaFechada();
    public Dealer::setCartaFechada();
};

Carta getCartaFechada(){
    return cartaFechada;
}
void setCartaFechada(Carta carta){
    cartaFechada = Carta(carta.naipe, carta.numeroCarta, carta.valor, carta.bool);
}
